package com.company;



public class Kadanealgo {
    
static int MaxSubArray(int a[]){
    int size=a.length;
    int max=a[0];
    int cur=a[0];
    
    for(int i=0;i<size;i++){
        
        cur=Math.max(cur+a[i],a[i]);
        max=Math.max(max,cur);
        
        
    }
    
    return max;
    
    
}    
    
    public static  void main(String args[]){
        
        
        
        
        
        
        
    }
}
